{
  "name": "gept-backend",
  "version": "1.0.12",
  "main": "server.js",
  "author": "Lucas Tavares",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored && webpack --config webpack.config.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --exit-child src/server.ts",
    "start": "node dist/server.js",
    "test": "export NODE_ENV=test && jest --detectOpenHandles --watch",
    "migration:create": "npx typeorm migration:create ./src/infra/postgres/migrations/CreateDefaultUser",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/loaders/postgres.ts",
    "migration:undo": "typeorm-ts-node-commonjs migration:revert -d ./src/loaders/postgres.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.6",
    "@types/http-terminator": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^15.3.1",
    "@types/uuid": "^9.0.2",
    "@types/yup": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "@webpack-cli/serve": "^1.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "git-commit-msg-linter": "^3.2.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "mrm": "^3.0.9",
    "prettier": "^2.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "env-var": "^6.3.0",
    "express": "^4.17.1",
    "http-terminator": "^2.0.3",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "yup": "^0.29.3"
  }
}